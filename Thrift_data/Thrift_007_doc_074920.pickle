                             <td rowspan="1" colspan="1">\n                                <i>\n                                  <b>\n                                    <a href="http://en.wikipedia.org/wiki/Pacific_Daylight_Time" title="Pacific Daylight Time" shape="rect">PDT</a>\n                                    <b></b>\n                                    (\n                                    <a href="http://en.wikipedia.org/wiki/UTC-7" title="UTC-7" shape="rect">UTC-7</a>\n                                    <b></b>\n                                    )\n                                  </b>\n                                </i>\n                              </td>\n                            </tr>\n                            <tr class="mergedtoprow">\n                              <th rowspan="1" colspan="1">\n                                <i>\n                                  <b>\n                                    <a href="http://en.wikipedia.org/wiki/ZIP_code" title="ZIP code" shape="rect">ZIP code</a>\n                                  </b>\n                                </i>\n                              </th>\n                              <td class="adr" rowspan="1" colspan="1">\n                                <i>\n                                  <b>\n                                    <span class="postal-code">942xx, 958xx</span>\n                                  </b>\n                                </i>\n                              </td>\n                            </tr>\n                            <tr class="mergedrow">\n                              <th rowspan="1" colspan="1">\n                                <i>\n                                  <b>\n                                    <a href="http://en.wikipedia.org/wiki/Telephone_numbering_plan" title="Telephone numbering plan" shape="rect">\n                                      Area code\n                                      <b></b>\n                                