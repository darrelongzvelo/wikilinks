                                                                                                                                                                                                                                                                                                          <table width="100%" border="0" cellspacing="0" cellpadding="6">\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    <tr>\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      <td style="border:1px inset" class="alt2" rowspan="1" colspan="1">\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        <div>\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          Originally Posted by\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          <strong>ElvisL1ves</strong>\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          <a rel="nofollow" href="showthread.php?s=8ab58bae7da2379787805d7fdb118480&amp;p=15114469#post15114469" shape="rect">\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            <img alt="View Post" border="0" src="images/buttons/viewpost.gif" class="inlineimg"></img>\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          </a>\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        </div>\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        <div>\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          So what\'s the etymology - that upstate NY and SC are literally at higher elevations than the coastal cities, or that they\'re upriver?\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          <br clear="none"></br>\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          <br clear="none"></br>\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          Side note:  Columbus native here.  There\'s northern,\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          <i>central</i>\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          , and southern Ohio to us.  We resent being lumped in with the\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          <i>real </i>\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          rednecks in southern Ohio, even though we say the world &quot;Ahia&quot; in the same Appalachian accent they use.  We grew up hearing West Virginian jokes, too.\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        </div>\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      </td>\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    </tr>\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  </table>\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                </div>\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                Both, and also in shape, what with South Carolina being shaped like a piece of pie.  Mmm, pie.\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              </div>\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                  