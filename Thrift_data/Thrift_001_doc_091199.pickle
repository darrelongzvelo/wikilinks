n Date: Jun 2002</div>\n                                                                                                                                                                                                                                                                                      </div>\n                                                                                                                                                                                                                                                                                    </td>\n                                                                                                                                                                                                                                                                                  </tr>\n                                                                                                                                                                                                                                                                                  <tr>\n                                                                                                                                                                                                                                                                                    <td id="td_post_15113675" class="alt1" rowspan="1" colspan="1">\n                                                                                                                                                                                                                                                                                      <div id="post_message_15113675">\n                                                                                                                                                                                                                                                                                        <div style="margin:20px; margin-top:5px; ">\n                                                                                                                                                                                                                                                                                          <div style="margin-bottom:2px" class="smallfont">Quote:</div>\n                                                                                                                                                                                                                                                                                          <table width="100%" border="0" cellspacing="0" cellpadding="6">\n                                                                                                                                                                                                                                                                                            <tr>\n                                                                                                                                                                                                                                                                                              <td style="border:1px inset" class="alt2" rowspan="1" colspan="1">\n                                                                                                                                                                                                                                                                                                <div>\n                                                                                                                                                                                                                                                                                                  Originally Posted by\n                                                                                                                                                                                                                                                                                                  <strong>Athena</strong>\n                                                                                                                                                                                                                                                                                                  <a rel="nofollow" href="showthread.php?s=8ab58bae7da2379787805d7fdb118480&amp;p=15113077#post15113077" shape="rect">\n                                                                                                                                                                                                                                                                                                    <img alt="View Post" border="0" src="images/buttons/viewpost.gif" class="inlineimg"></img>\n                                                                                                                                                                                                                                                                                                  </a>\n                                                                                                                                                                                                                                                                                                </div>\n                                                                                                                                                                                                                                                                                                <div>\n                                                                                                                                                                                                                                                                                                  True, but I\'ve never heard the term &quot;Upstate&quot; here.\n                                                                                                                                                                                                                                                                                                  <br clear="none"></br>\n                                                                                                                                                                                                                                                                                                  <br clear="none"></br>\n                                                                                                                                                                                                                                                                                                  There\'s &quot;Downstate&quot;, which describes everything in the southern peninsula.\n                                                                                                                                                                                                                                                                                                  <br clear="none"></br>\n                                                                                                                                                                                                                                                                                                  <br clear="none"></br>\n                                                                                                                                                                                                                                                                                                  There\'s &quot;Northern Michigan&quot;, which describes everything in the northern half of the southern peninsula.\n                                                                                                                                                                                                                                                                                                  <br clear="none"></br>\n                                                                                                                                                                                                                                                                                                  <br clear="none"></br>\n                                                                                                                                                                                                                                                                                                  And then there\'s the &quot;Upper Peninsula&quot; aka &quot;UP&quot;, which is the northern peninsula. And yeah, we think it\'s funny that we\'re north of &quot;Northern Michigan.&quot;\n                                                                                                                                                                                                                                                                                                </div>\n                                                                                                                                                                                                                                                                                              </td>\n                                                                                                                                                                                                                                                                                            </tr>\n                                                                                                                                                                                                                                                                                          </table>\n                                                                                                                                                                                                                                                                                        </div>\n                                                                                                                                                                                                                                                                                        As a kid I can remember my confusion as to everything outside of Detroit (when it was the 5th largest city in the US) as &quot;upstate&quot;. Maybe it was &quot;out of state&quot;, can\'t actually recall, but there was a clear distinction between Detroit and the rest of Michigan.\n                                                                                                                                                                                                                                                                                      </div>\n                                                                                                                                                                                                                                                                                      <div style="margin-top: 10px" align="right">\n                                                                                                                                                                                                                                                                                        <a rel="nofollow" href="newreply.php?s=8ab58bae7da2379787805d7fdb118480&amp;do=newreply&amp;p=15113675" shape="rect">\n                                                                                                                                                                                                                                                                                          <img border="0" alt="Reply With Quote" src="images/buttons/quote.gif"></img>\n                                                                                                                                                                                                                                                                                        </a>\n                                                                                                                                                                                                                                                                                      </div>\n                                                                                                                                                                                                                                                                                    </td>\n                                                                                                                                                                                                                                                                                  </tr>\n                                                                                                                                                                                                                                                                                </table>\n                                                                                                                                                                                                                                                                                <div style="display:none;" id="postmenu_15113675_menu" class="vbmenu_popup">\n                                                                                                                                                                                                                                                                                  <table border="0" cellspacing="1" cellpadding="4">\n                                                                                                                                                                                                                                                                                    <tr>\n                                                                                                                                                                                                                                                                                      <td class="thead" rowspan="1" colspan="1">R. P. McMurphy</td>\n                                                                                                                                                                                                                                                                                    </tr>\n                                          