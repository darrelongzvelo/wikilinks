                                                                                                 <a rel="nofollow" href="showthread.php?s=8ab58bae7da2379787805d7fdb118480&amp;p=15114069#post15114069" shape="rect">\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              <img alt="View Post" border="0" src="images/buttons/viewpost.gif" class="inlineimg"></img>\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            </a>\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          </div>\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          <div>\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            Two minor nitpicks:\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            <br clear="none"></br>\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            <br clear="none"></br>\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            Everything north of Westchester County, as\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            <b>Polycarp</b>\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            suggests above.\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          </div>\n                                                                                                                                                                             