ithOYDrlF8JlC1M=\'); f.appendChild(s);f.submit(); };return false;" href="/posts/destroy/22059448" shape="rect">\n                    Delete\n</a>\n                </li>\n                <li>\n                  <a data-posterous-tag-form="true" data-posterous-modal="true" data-post-id="22059448" href="http://posterous.com/posts/22059448/tags/edit_multiple" shape="rect">Tags</a>\n                </li>\n                <li>\n                  <a data-posterous-modal="true" href="//posterous.com/autopost/crosspost?post_id=22059448" shape="rect">Autopost</a>\n                </li>\n              </ul>\n            </div>\n            <a href="http://actionpacked.posterous.com/daring-idea-for-safe-sex" class="date-bubble" shape="rect">\n              <span class="month">7 Jul</span>\n              <span class="year">2010</span>\n            </a>\n            <h2 id="posttitle_22059448" class="post-title">\n              <a href="http://actionpacked.posterous.com/daring-idea-for-safe-sex" shape="rect">Daring idea for safe sex</a>\n            </h2>\n            <div class="copy">\n              <div class="posterous_bookmarklet_entry">\n                <object width="500" height="417">\n                  <param value="http://www.youtube.com/v/ouufyoz9vQs&amp;hl=en&amp;fs=1" name="movie" valuetype="data"></param>\n                  <param value="window" name="wmode" valuetype="data"></param>\n                  <param value="true" name="allowFullScreen" valuetype="data"></param>\n                  <param value="always" name="allowscriptaccess" valuetype="data"></param>\n                  <embed width="500" height="417" wmode="window" type="application/x-shockwave-flash" src="http://www.youtube.com/v/ouufyoz9vQs&amp;hl=en&amp;fs=1"></embed>\n                </object>\n                <div class="posterous_quote_citation">\n                  via\n                  <a href="http://www.youtube.com/watch?v=ouufyoz9vQs" shape="rect">youtube.com</a>\n                </div>\n                <p>Instead of focusing on the dangers of unprotected sex, the Stockholm County AIDS Prevention Programme distributed 100,000 condoms with an unique number and a URL for people to upload the stories about how did they use it.</p>\n                <p>Another Promo &amp; Activation Gold Lion, this time for\xc3\x82\xc2\xa0Charities, Public Health &amp; Safety, Public Awareness Messages. It also won a Promo &amp; Activation Silver Lion for\xc3\x82\xc2\xa0Best Integrated Campaign Led by Promotion and Activation.</p>\n              </div>\n            </div>\n          </div>\n          <ul class="post-meta-data clearfix">\n            <li class="tags">\n              <a href="/tag/awardwinning" shape="rect">award-winning</a>\n              <a href="/tag/casestudy" shape="rect">case study</a>\n              <a href="/tag/experiential" shape="rect">experiential</a>\n              <a href="/tag/field" shape="rect">field</a>\n              <a href="/tag/integrated" shape="rect">integrated</a>\n              <a href="/tag/promo" shape="rect">promo</a>\n              <a href="/tag/usergenerated" shape="rect">user-generated</a>\n            </li>\n            <li>\n              <a href="http://actionpacked.posterous.com/daring-idea-for-safe-sex" shape="rect">Permalink</a>\n            </li>\n            <li>\n              <a href="http://actionpacked.posterous.com/daring-idea-for-safe-sex#comment" id="comment_link_22059448" shape="rect">0 Comments</a>\n            </li>\n            <li class="retweet-button clearfix">\n              <div class="posterous_tweet_button">\n                <a \n                data-lang="en" data-related="posterous:Posterous is the place to post everything." data-count="horizontal" data-text="Daring idea for safe sex" data-via="danicomar" data-counturl="http://actionpacked.posterous.com/daring-idea-for-safe-sex" data-url="http://post.ly/lxwD" href="http://twitter.com/share" class="posterous_tweet_button twitter-share-button" shape="rect">\n                  Tweet\n</a>\n              </div>\n            </li>\n            <li class="fb-like-button">\n              <fb:like ref="" colorscheme="light" font="lucida grande" action="like" width="" show_faces="true" layout="button_count" href="http://actionpacked.posterous.com/daring-idea-for-safe-sex"></fb:like>\n            </li>\n          </ul>\n        </div>\n        <div id="postunit_21747410" class="post clearfix">\n          <div id="postbox_21747410" class="post-content">\n            <div id="editunit_21747410" class="editbox clearfix">\n              <ul data-posterous-post-id="21747410" data-posterous-edit-box="true" class="mini_commands posterous_edit_box posterous_edit_box_hidden">\n                <li>\n                  <a href="http://posterous.com/posts/edit/21747410" shape="rect">Edit</a>\n                </li>\n                <li>\n                  <a \n                  onclick="if (confirm(\'Are you sure you want to delete this post?\')) { var f = document.createElement(\'form\'); f.style.display = \'none\'; this.parentNode.appendChild(f); f.method = \'POST\'; f.action = this.href;var m = document.createElement(\'input\'); m.setAttribute(\'type\', \'hidden\'); m.setAttribute(\'name\', \'_method\'); m.setAttribute(\'value\', \'delete\'); f.appendChild(m);var s = document.createElement(\'input\'); s.setAttribute(\'type\', \'hidden\'); s.setAttribute(\'name\', \'authenticity_token\'); s.setAttribute(\'value\', \'90c9hgs1q/IitF8iAe2+m4Jgtp+ZithOYDrlF8JlC1M=\'); f.appendChild(s);f.submit(); };return false;" href="/posts/destroy/21747410" shape="rect">\n                    Delete\n</a>\n                </li>\n                <li>\n                  <a data-posterous-tag-form="true" data-posterous-modal="true" data-post-id="21747410" href="http://posterous.com/posts/21747410/tags/edit_multiple" shape="rect">Tags</a>\n                </li>\n                <li>\n                  <a data-posterous-modal="true" href="//posterous.com/autopost/crosspost?post_id=21747410" shape="rect">Autopost</a>\n                </li>\n              </ul>\n            </div>\n            <a href="http://actionpacked.posterous.com/the-first-interactive-horror-movie" class="date-bubble" shape="rect">\n              <span class="month">2 Jul</span>\n              <span class="year">2010</span>\n            </a>\n            <h2 id="posttitle_21747410" class="post-title">\n              <a href="http://actionpacked.posterous.com/the-first-interactive-horror-movie" shape="rect">The first interactive horror movie</a>\n            </h2>\n            <div class="copy">\n              <div class="posterous_bookmarklet_entry">\n                <object width="500" height="417">\n                  <param value="http://www.youtube.com/v/qe9CiKnrS1w&amp;hl=en&amp;fs=1" name="movie" valuetype="data"></param>\n                  <param value="window" name="wmode" valuetype="data"></param>\n                  <param value="true" name="allowFullScreen" valuetype="data"></param>\n                  <param value="always" name="allowscriptaccess" valuetype="data"></param>\n                  <embed width="500" height="417" wmode="window" type="application/x-shockwave-flash" src="http://www.youtube.com/v/qe9CiKnrS1w&amp;hl=en&amp;fs=1"></embed>\n                </object>\n                <div class="posterous_quote_citation">\n                  via\n                  <a href="http://www.youtube.com/watch?v=qe9CiKnrS1w" shape="rect">youtube.com</a>\n                </div>\n                <p>\n                  With this digital version of \xc3\xa2\xe2\x82\xac\xcb\x9c\n                  <a href="http://en.wikipedia.org/wiki/Choose_Your_Own_Adventure" shape="rect">Choose your Own Adventure</a>\n                  \xc3\xa2\xe2\x82\xac\xe2\x84\xa2 13th Street Horror TV channel engaged its audience at the cinemas.\n                </p>\n                <p>People who opted for participating at the beginning of the movie would receive a call from the movie star in danger and he or she would help her to scape, creating a different film each time.</p>\n                <p>\n                  \xc3\xa2\xe2\x82\xac\xcb\x9cLast Call\xc3\xa2\xe2\x82\xac\xe2\x84\xa2 just won a Promo &amp; Activation Gold Lion for Best Use of Other Digital Media in a Promotional Campaign. I\xc3\xa2\xe2\x82\xac\xe2\x84\xa2ll keep posting more of the latest\n                  <a href="http://www.canneslions.com/work/promo/" shape="rect">Cannes winners</a>\n                  .\n                </p>\n              </div>\n            </div>\n          </div>\n          <ul class="post-meta-data clearfix">\n            <li class="tags">\n              <a href="/tag/awardwinning" shape="rect">award-winning</a>\n              <a href="/tag/casestudy" shape="rect">case study</a>\n              <a href="/tag/digital" shape="rect">digital</a>\n              <a href="/tag/experiential" shape="rect">experiential</a>\n            </li>\n            <li>\n              <a href="http://actionpacked.posterous.com/the-first-interactive-horror-movie" shape="rect">Permalink</a>\n            </li>\n            <li>\n              <a href="http://actionpacked.posterous.com/the-first-interactive-horror-movie#comment" id="comment_link_21747410" shape="rect">0 Comments</a>\n            </li>\n            <li class="retweet-button clearfix">\n              <div class="posterous_tweet_button">\n                <a \n                data-lang="en" data-related="posterous:Posterous is the place to post everything." data-count="horizontal" data-text="The first interactive horror movie" data-via="danicomar" data-counturl="http://actionpacked.posterous.com/the-first-interactive-horror-movie" data-url="http://post.ly/lSOw" href="http://twitter.com/share" class="posterous_tweet_button twitter-share-button" shape="rect">\n                  Tweet\n</a>\n              </div>\n            </li>\n            <li class="fb-like-button">\n              <fb:like ref="" colorscheme="light" font="lucida grande" action="like" width="" show_faces="true" layout="button_count" href="http://actionpacked.posterous.com/the-first-interactive-horror-movie"></fb:like>\n            </li>\n          </ul>\n        </div>\n        <div id="postunit_21104186" class="post clearfix">\n          <div id="postbox_21104186" class="post-content">\n            <div id="editunit_21104186" class="editbox clearfix">\n              <ul data-posterous-post-id="21104186" data-posterous-edit-box="true" class="mini_commands posterous_edit_box posterous_edit_box_hidden">\n                <li>\n                  <a href="http://posterous.com/posts/edit/21104186" shape="rect">Edit</a>\n                </li>\n                <li>\n                  <a \n                  onclick="if (confirm(\'Are you sure you want to delete this post?\')) { var f = document.createElement(\'form\'); f.style.display = \'none\'; this.parentNode.appendChild(f); f.method = \'POST\'; f.action = this.href;var m = document.createElement(\'input\'); m.setAttribute(\'type\', \'hidden\'); m.setAttribute(\'name\', \'_method\'); m.setAttribute(\'value\', \'delete\'); f.appendChild(m);var s = document.createElement(\'input\'); s.setAttribute(\'type\', \'hidden\'); s.setAttribute(\'name\', \'authenticity_token\'); s.setAttribute(\'value\', \'90c9hgs1q/IitF8iAe2+m4Jgtp+ZithOYDrlF8JlC1M=\'); f.appendChild(s);f.submit(); };return false;" href="/posts/destroy/21104186" shape="rect">\n                    Delete\n</a>\n                </li>\n                <li>\n                  <a data-posterous-tag-form="true" data-posterous-modal="true" data-post-id="21104186" href="http://posterous.com/posts/21104186/tags/edit_multiple" shape="rect">Tags</a>\n                </li>\n                <li>\n                  <a data-posterous-modal="true" href="//posterous.com/autopost/crosspost?post_id=21104186" shape="rect">Autopost</a>\n                </li>\n              </ul>\n            </div>\n            <a href="http://actionpacked.posterous.com/virtually-all-cannes-grand-prix-use-activatio" class="date-bubble" shape="rect">\n              <span class="month">25 Jun</span>\n              <span class="year">2010</span>\n            </a>\n            <h2 id="posttitle_21104186" class="post-title">\n              <a href="http://actionpacked.posterous.com/virtually-all-cannes-grand-prix-use-activatio" shape="rect">Virtually all Cannes Grand Prix use activation</a>\n            </h2>\n            <div class="copy">\n              <p>It couldn\xc3\xa2\xe2\x82\xac\xe2\x84\xa2t be a better time. Activation is coming of age. I said many times that the most exciting work in the past few years used some sort of direct consumer engagement like KitKat Mail, The Best Job in the World or Earth Hour. But this year results are speaking loud and clear.</p>\n              <p>Practically all of the Grand Prix winners are activation in nature. And not only the usual suspects as Promo or Media Lions, but also the least expected categories like Design or even Press. Let\xc3\xa2\xe2\x82\xac\xe2\x84\xa2s take a look:</p>\n              <p></p>\n              <p class="readmore">\n                <a href="http://actionpacked.posterous.com/virtually-all-cannes-grand-prix-use-activatio#more" shape="rect">Read the rest of this post \xc2\xbb</a>\n              </p>\n            </div>\n          </div>\n          <ul class="post-meta-data clearfix">\n            <li class="tags">\n              <a href="/tag/awardwinning" shape="rect">award-winning</a>\n              <a href="/tag/foodforthought" shape="rect">food for thought</a>\n            </li>\n            <li>\n              <a href="http://actionpacked.posterous.com/virtually-all-cannes-grand-prix-use-activatio" shape="rect">Permalink</a>\n            </li>\n            <li>\n              <a href="http://actionpacked.posterous.com/virtually-all-cannes-grand-prix-use-activatio#comment" id="comment_link_21104186" shape="rect">0 Comments</a>\n            </li>\n            <li class="retweet-button clearfix">\n              <div class="posterous_tweet_button">\n                <a \n                data-lang="en" data-related="posterous:Posterous is the place to post everything." data-count="horizontal" data-text="Virtually all Cannes Grand Prix use activation" data-via="danicomar" data-counturl="http://actionpacked.posterous.com/virtually-all-cannes-grand-prix-use-activatio" data-url="http://post.ly/l5Hc" href="http://twitter.com/share" class="posterous_tweet_button twitter-share-button" shape="rect">\n                  Tweet\n</a>\n              </div>\n            </li>\n            <li class="fb-like-button">\n              <fb:like ref="" colorscheme="light" font="lucida grande" action="like" width="" show_faces="true" layout="button_count" href="http://actionpacked.posterous.com/virtually-all-cannes-grand-prix-use-activatio"></fb:like>\n            </li>\n          </ul>\n        </div>\n        <div id="postunit_20857010" class="post clearfix">\n          <div id="postbox_20857010" class="post-content">\n            <div id="editunit_20857010" class="editbox clearfix">\n              <ul data-posterous-post-id="20857010" data-posterous-edit-box="true" class="mini_commands posterous_edit_box posterous_edit_box_hidden">\n                <li>\n                  <a href="http://posterous.com/posts/edit/20857010" shape="rect">Edit</a>\n                </li>\n                <li>\n                  <a \n                  onclick="if (confirm(\'Are you sure you want to delete this post?\')) { var f = document.createElement(\'form\'); f.style.display = \'none\'; this.parentNode.appendChild(f); f.method = \'POST\'; f.action = this.href;var m = document.createElement(\'input\'); m.setAttribute(\'type\', \'hidden\'); m.setAttribute(\'name\', \'_method\'); m.setAttribute(\'value\', \'delete\'); f.appendChild(m);var s = document.createElement(\'input\'); s.setAttribute(\'type\', \'hidden\'); s.setAttribute(\'name\', \'authenticity_token\'); s.setAttribute(\'value\', \'90c9hgs1q/IitF8iAe2+m4Jgtp+ZithOYDrlF8JlC1M=\'); f.appendChild(s);f.submit(); };return false;" href="/posts/destroy/20857010" shape="rect">\n                    Delete\n</a>\n                </li>\n                <li>\n                  <a data-posterous-tag-form="true" data-posterous-modal="true" data-post-id="20857010" href="http://posterous.com/posts/20857010/tags/edit_multiple" shape="rect">Tags</a>\n                </li>\n                <li>\n                  <a data-posterous-modal="true" href="//posterous.com/autopost/crosspost?post_id=20857010" shape="rect">Autopost</a>\n                </li>\n              </ul>\n            </div>\n            <a href="http://actionpacked.posterous.com/the-2010-cannes-promo-and-activation-lions-gr" class="date-bubble" shape="rect">\n              <span class="month">23 Jun</span>\n              <span class="year">2010</span>\n            </a>\n            <h2 id="posttitle_20857010" class="post-title">\n              <a href="http://actionpacked.posterous.com/the-2010-cannes-promo-and-activation-lions-gr" shape="rect">The 2010 Cannes Promo &amp; Activation Lions Grand Prix</a>\n            </h2>\n            <div class="copy">\n              <div class="posterous_bookmarklet_entry">\n                <object width="500" height="366">\n                  <param value="http://www.youtube.com/v/xQWSsfluZI8&amp;rel=1&amp;color1=d6d6d6&amp;color2=f0f0f0&amp;border=1&amp;fs=1&amp;hl=en&amp;autoplay=0&amp;showinfo=0&amp;iv_load_policy=3&amp;showsearch=0&amp;hd=1" name="movie" valuetype="data"></param>\n                  <param value="true" name="allowFullScreen" valuetype="data"></param>\n                  <embed width="500" height="366" wmode="transparent" type="application/x-shockwave-flash" src="http://www.youtube.com/v/xQWSsfluZI8&amp;rel=1&amp;color1=d6d6d6&amp;color2=f0f0f0&amp;border=1&amp;fs=1&amp;hl=en&amp;autoplay=0&amp;showinfo=0&amp;iv_load_policy=3&amp;showsearch=0&amp;hd=1"></embed>\n                  <param value="transparent" name="wmode" valuetype="data"></param>\n                </object>\n                <div class="posterous_quote_citation">\n                  via\n                  <a href="http://theinspirationroom.com/daily/2009/thanksgiving-replay-fueled-by-gatorade/" shape="rect">theinspirationroom.com</a>\n                </div>\n                <p>\n                  All the\n                  <a target="_blank" href="http://www.canneslions.com/work/promo/index.cfm?award=1" shape="rect">Promo &amp; Activation Lions winners</a>\n                  are now online and Gatorade took the Grand Prix with an inspired integrated campaign/event.\n                </p>\n                <p>\n                  Since 7 out of 10 adults don\xc3\xa2\xe2\x82\xac\xe2\x84\xa2t exercise regularly and, therefore, don\xc3\xa2\xe2\x82\xac\xe2\x84\xa2t consume Gatorade as often as before, they had to reignite their passion for sports. To do so, they sponsored an event that reunited the \xc3\xa2\xe2\x82\xac\xe2\x80\x9dnow over 30 years old\xc3\xa2\xe2\x82\xac\xe2\x80\x9d players of two rival American football teams which last legendary match ended up in a tie 17 years ago.\n                </p>\n                <p>\n                  Above is\n                  <a target="_blank" href="http://www.youtube.com/watch?v=xQWSsfluZI8" shape="rect">the trailer</a>\n                  but better watch\n                  <a target="_blank" href="http://www.canneslions.com/work/promo/index.cfm?award=1" shape="rect">the case study</a>\n                  at Cannes website.\n                </p>\n                <p>\n                  Also take a look at the\n                  <a target="_blank" href="http://www.canneslions.com/work/promo/index.cfm?award=2" shape="rect">Gold winners</a>\n                  ,\n                  <a target="_blank" href="http://www.canneslions.com/work/promo/index.cfm?award=3" shape="rect">Silver winners</a>\n                  and\n                  <a target="_blank" href="http://www.canneslions.com/work/promo/index.cfm?award=4" shape="rect">Bronze winners</a>\n                  .\n                </p>\n                <p>\n                  Many of them \xc3\xa2\xe2\x82\xac\xe2\x80\x9dlike\n                  <a href="http://actionpacked.posterous.com/football-fans-pranked-by-girlfriends-and-boss" shape="rect">Auditorium</a>\n                  ,\n                  <a href="http://actionpacked.posterous.com/from-the-creators-of-hbo-voyeur" shape="rect">The Cube</a>\n                  ,\n                  <a href="http://actionpacked.posterous.com/the-north-face-red-flags-now-on-video" shape="rect">Red Flag Movement</a>\n                  ,\n                  <a href="http://actionpacked.posterous.com/the-worlds-biggest-signpost-16" shape="rect">World\xc3\xa2\xe2\x82\xac\xe2\x84\xa2s Biggest Signpost</a>\n                  ,\n                  <a href="http://actionpacked.posterous.com/why-cheat-when-you-can-teletransport" shape="rect">Teletransporter</a>\n                  ,\n                  <a href="http://actionpacked.posterous.com/the-73000-bar-bill" shape="rect">$73,000 Bar Tab</a>\n                  and\n                  <a href="http://actionpacked.posterous.com/smile-youre-buying-a-burger" shape="rect">Whopper Face</a>\n                  \xc3\xa2\xe2\x82\xac\xe2\x80\x9d we\xc3\xa2\xe2\x82\xac\xe2\x84\xa2ve covered before.\xc3\x82\xc2\xa0I\xc3\xa2\xe2\x82\xac\xe2\x84\xa2ll be writing about the rest of the winners soon.\n                </p>\n              </div>\n            </div>\n          </div>\n          <ul class="post-meta-data clearfix">\n            <li class="tags">\n              <a href="/tag/awardwinning" shape="rect">award-winning</a>\n              <a href="/tag/event" shape="rect">event</a>\n              <a href="/tag/experiential" shape="rect">experiential</a>\n              <a href="/tag/integrated" shape="rect">integrated</a>\n              <a href="/tag/promo" shape="rect">promo</a>\n              <a href="/tag/sponsorship" shape="rect">sponsorship</a>\n            </li>\n            <li>\n              <a href="http://actionpacked.posterous.com/the-2010-cannes-promo-and-activation-lions-gr" shape="rect">Permalink</a>\n            </li>\n            <li>\n              <a href="http://actionpacked.posterous.com/the-2010-cannes-promo-and-activation-lions-gr#comment" id="comment_link_20857010" shape="rect">0 Comments</a>\n            </li>\n            <li class="retweet-button clearfix">\n              <div class="posterous_tweet_button">\n                <a \n                data-lang="en" data-related="posterous:Posterous is the place to post everything." data-count="horizontal" data-text="The 2010 Cannes Promo &amp; Activation Lions Grand Prix" data-via="danicomar" data-counturl="http://actionpacked.posterous.com/the-2010-cannes-promo-and-activation-lions-gr" data-url="http://post.ly/kSfn" href="http://twitter.com/share" class="posterous_tweet_button twitter-share-button" shape="rect">\n                  Tweet\n</a>\n              </div>\n            </li>\n            <li class="fb-like-button">\n              <fb:like ref="" colorscheme="light" font="lucida grande" action="like" width="" show_faces="true" layout="button_count" href="http://actionpacked.posterous.com/the-2010-cannes-promo-and-activation-lions-gr"></fb:like>\n            </li>\n          </ul>\n        </div>\n        <div id="postunit_20755658" class="post clearfix">\n          <div id="postbox_20755658" class="post-content">\n            <div id="editunit_20755658" class="editbox clearfix">\n              <ul data-posterous-post-id="20755658" data-posterous-edit-box="true" class="mini_commands posterous_edit_box posterous_edit_box_hidden">\n                <li>\n                  <a href="http://posterous.com/posts/edit/20755658" shape="rect">Edit</a>\n                </li>\n                <li>\n                  <a \n                  onc